{% if tweets %}
{% for tweet in tweets %}
<div class="tweet">

  <div class="tweet__profile">
    USER -- @UserOne
  </div>

  <div class="tweet__content">
    {{ tweet.content }}
  </div>

  <a href="#" class="btn tweet__delete-btn" data-tweetid="{{ tweet._id }}"><i class="fa-solid fa-trash-can icon"></i></a>

</div>
{% endfor %}
{% endif %}